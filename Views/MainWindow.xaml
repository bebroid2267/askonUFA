<Window x:Class="askonUFA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:askonUFA"
        mc:Ignorable="d"
        xmlns:vm ="clr-namespace:askonUFA.ViewModels" 
        d:DataContext="{d:DesignInstance Type=vm:MainViewModel}"
        Title="MainWindow" 
        Height="450" 
        Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <StackPanel>
                
            </StackPanel>
            
            <TreeView x:Name="tree" ItemsSource="{Binding Objects}" Width="100">
                <TreeView.ItemTemplate>
                    <HierarchicalDataTemplate DataType="x:Type vm:MainViewModel"
                                              ItemsSource ="{Binding Attributess}">
                        
                            <TextBlock Text="{Binding Type}" />
                    </HierarchicalDataTemplate>
                </TreeView.ItemTemplate>
            </TreeView>
        </StackPanel>
    </Grid>
</Window>
